<% if group.blank? %>
  <%= l(:label_none) %>
<% elsif group.is_a?(Integer) && query.group_by == 'parent_id' %>
  <% parent = Issue.find_by(id: group) %>
  <% if parent %>
    <%= link_to "##{parent.id}: #{parent.subject}", issue_path(parent) %>
  <% else %>
    #<%= group %>
  <% end %>
<% elsif group.is_a?(Array) %>
  <%= group.map {|v| column_content(column, issue, v)}.join(', ').html_safe %>
<% else %>
  <%= column_content(column, issue, group) %>
<% end %>
